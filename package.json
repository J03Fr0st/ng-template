{
  "name": "ng-template",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "changeset": "changeset",
    "version": "changeset version",
    "lint": "biome lint --files-ignore-unknown=true ./src",
    "lint:fix": "biome lint --write --files-ignore-unknown=true ./src",
    "format": "biome format --write --files-ignore-unknown=true ./src",
    "format:check": "biome format --files-ignore-unknown=true ./src",
    "check": "biome check --files-ignore-unknown=true ./src",
    "check:fix": "biome check --write --files-ignore-unknown=true ./src",
    "ci": "npm run check && npm run build && npm run test",
    "prepare": "husky"
  },
  "private": true,
  "packageManager": "npm@11.4.2",
  "dependencies": {
    "@angular/common": "^21.0.0",
    "@angular/compiler": "^21.0.0",
    "@angular/core": "^21.0.0",
    "@angular/forms": "^21.0.0",
    "@angular/platform-browser": "^21.0.0",
    "@angular/router": "^21.0.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular/build": "^21.0.0",
    "@angular/cli": "^21.0.0",
    "@angular/compiler-cli": "^21.0.0",
    "@biomejs/biome": "2.3.6",
    "@changesets/cli": "^2.29.7",
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "angular-eslint": "20.6.0",
    "husky": "^9.1.7",
    "jsdom": "^27.1.0",
    "lint-staged": "^16.2.7",
    "typescript": "~5.9.2",
    "typescript-eslint": "8.46.3",
    "vitest": "^4.0.8"
  }
}
